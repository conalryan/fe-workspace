{
  "name": "fe-workspace",
  "version": "0.0.1",
  "author": "Conal Ryan",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "bootstrap": "pnpm i && pnpm build",
    "build": "lerna run build --parallel",
    "clean": "pnpm clean:dist && pnpm clean:node_modules && pnpm clean:nx",
    "clean:dist": "pnpm -r exec rm -rf ./dist ./.__mf__temp",
    "clean:node_modules": "pnpm -r exec rm -rf ./node_modules",
    "clean:nx": "rm -rf .nx",
    "dev": "lerna run dev --parallel",
    "dev:lit": "lerna run dev --parallel --scope=\"lit-*\"",
    "dev:module-federation": "lerna run dev --parallel --scope=react-mf-mui-tan-basic-host --scope=react-mf-mui-tan-basic-remote --scope=react-mui-tan-host --scope=react-mui-tan-remote",
    "dev:react": "lerna run dev --parallel --scope=\"react-*\"",
    "lint": "lerna run lint --parallel",
    "lint:fix": "lerna run lint:fix --parallel",
    "format": "lerna run format --parallel"
  },
  "devDependencies": {
    "lerna": "^9.0.0"
  },
  "packageManager": "pnpm@9.13.2+sha512.88c9c3864450350e65a33587ab801acf946d7c814ed1134da4a924f6df5a2120fd36b46aab68f7cd1d413149112d53c7db3a4136624cfd00ff1846a0c6cef48a"
}
